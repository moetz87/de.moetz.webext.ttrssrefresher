{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": false,
        "noResolve": false,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "outDir": "build/tsc/"
    },
    "include": [
        "**/*.ts"
    ]
}